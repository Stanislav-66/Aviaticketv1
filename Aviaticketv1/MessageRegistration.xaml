<Window x:Class="Aviaticketv1.MessageRegistration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Aviaticketv1"
        mc:Ignorable="d"
        Title="AviaRofl" Height="164" Width="277" ResizeMode="NoResize" Icon="Resources/Image/Icons/Icon.png" Background="{Binding Backgr}">
    <Grid>
        
        <Label Margin="0,50,0,0" Content="{Binding mes1}" Foreground="{Binding Label}" HorizontalContentAlignment="Center" FontFamily="Times New Roman"/>
        <Label Margin="0,25,0,25" Content="{Binding mes2}" Foreground="{Binding Label}" HorizontalContentAlignment="Center" FontFamily="Times New Roman"></Label>
        <Button Template="{StaticResource StyleButton_s}" Margin="0,60,0,0" Background="White" Content="{Binding butcon}" Width="100" FontFamily="Times New Roman" Height="25" Click="Button_Click"></Button>
        
    </Grid>
</Window>
